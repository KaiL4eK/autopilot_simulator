<?xml version="1.0"?>

<launch>

	<include file="$(find stdr_robot)/launch/robot_manager.launch" />

	<node type="stdr_server_node" pkg="stdr_server" name="stdr_server" output="screen" args="$(find autopilot_simulator)/resources/maps/simple_room.yaml"/>

	<node pkg="tf" type="static_transform_publisher" name="world2map" args="0 0 0 0 0 0  world map 100" />

	<!-- <include file="$(find stdr_gui)/launch/stdr_gui.launch"/> -->

	<node pkg="stdr_robot" type="robot_handler" name="$(anon robot_spawn)" args="add $(find autopilot_simulator)/resources/quadcopter.xml 3 2 0" />

	<!-- <node pkg="key_teleop" type="key_teleop.py" name="key_teleop" output="screen"> -->
		<!-- <remap from="key_vel" to="robot0/cmd_vel" /> -->
	<!-- </node> -->

</launch>